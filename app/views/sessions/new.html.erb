
<%= render 'shared/content_title', title: 'Log In' %>
<div class='well'>
  <%= form_tag '/login' do %>
  <div class='form-group'>
    <%= label_tag :username %>
    <br/>
    <%= text_field_tag :username %>
  </div>
  <div class='form-group'>
    <%= label_tag :password %>
    <br/>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag 'Register', class: 'btn btn-primary' %>
  <% end %>
</div>